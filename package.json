{
    "name": "server",
    "version": "1.0.0",
    "main": "index.js",
    "author": "jc",
    "license": "MIT",
    "scripts": {
        "start": "cross-env NODE_ENV=prod PORT=8080 node server.js",
        "server-nodemon": "nodemon server.js",
        "server": "node server.js",
        "client": "npm run start --prefix client",
        "dev-nodemon": "cross-env NODE_ENV=dev concurrently \"npm run server-nodemon\" \"npm run client\"",
        "dev": "cross-env NODE_ENV=dev concurrently \"npm run server\" \"npm run client\"",
        "test": "cross-env NODE_ENV=test jest"
    },
    "dependencies": {
        "@google-cloud/storage": "^5.0.1",
        "@sendgrid/mail": "^7.6.2",
        "bcryptjs": "^2.4.3",
        "compression": "^1.7.4",
        "concurrently": "^5.2.0",
        "cookie-parser": "^1.4.5",
        "cors": "^2.8.5",
        "dotenv": "^8.2.0",
        "express": "^4.17.1",
        "express-validator": "^6.4.1",
        "helmet": "^3.22.0",
        "jsonwebtoken": "^8.5.1",
        "mailgun-js": "^0.22.0",
        "mongoose": "^5.9.12",
        "multer": "^1.4.2",
        "react-helmet-async": "^1.0.6",
        "socket.io": "^2.5.0",
        "uuid": "^8.1.0"
    },
    "devDependencies": {
        "cross-env": "^7.0.2",
        "jest": "^24.9.0",
        "nodemon": "^2.0.3",
        "supertest": "^4.0.2"
    },
    "jest": {
        "testEnvironment": "node",
        "coveragePathIgnorePatterns": [
            "/node_modules/"
        ],
        "testTimeout": 100000
